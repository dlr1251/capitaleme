---
export const prerender = true;
import Layout from '../../layouts/Layout.astro';
import BlogPageLatest from '../../components/features/blog/BlogPageLatest.astro';
import HomePageHero from '../../components/pages/home/HomePageHero.astro';
import AboutPageSection from '../../components/pages/about/AboutPageSection.astro';
import HomeRealEstateSection from '../../components/pages/home/HomeRealEstateSection';
import HomeVisaAssistanceSection from '../../components/pages/home/HomeVisaAssistanceSection';
import HomePageCLKR from '../../components/pages/home/HomePageCLKR.jsx';
import CalendlyInline from '../../components/shared/CalendlyInline.astro';
import HowWeWork from '../../components/shared/HowWeWork.astro';
import { getAllMenuData } from '../../utils/menuData.js';

const menuData = await getAllMenuData('en');
const allVisas = menuData?.allVisas || [];
const guides = menuData?.guides || [];
const featuredProperty = menuData?.featuredProperty || null;

---
<Layout title="Capital M Law - Home" menuData={menuData}>
	<main class="">        
        <HomePageHero/>
        <AboutPageSection/>
        <HomeVisaAssistanceSection visas={allVisas} guides={guides} lang="en" client:load />
        <HomeRealEstateSection featuredProperty={featuredProperty} features={[]} lang="en" client:load />
        <HomePageCLKR lang="en" client:load />
    		<!-- <CalendlyInline /> -->
        <HowWeWork lang="en"/>
		    <BlogPageLatest/>
	</main>
</Layout>

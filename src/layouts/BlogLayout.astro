---
import Navbar from '../components/Navbar.astro';
import Sidebar from '../components/Sidebar.astro';
import Layout from './Layout.astro';
interface Props {
	title: string;	
}

const { title } = Astro.props

---

<Layout title={title}>

	<Sidebar>			
		<h1>{title}</h1>
		<div class="max-w-screen-md mx-auto">
			<slot />
		</div>
		
	</Sidebar>		
</Layout>


